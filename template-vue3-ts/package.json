{
  "name": "vue-dsfr-ts-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "run-s test:unit test:ct:ci test:e2e:ci",
    "test:unit": "vitest --environment jsdom",
    "test:ct": "cypress open-ct",
    "test:ct:ci": "cypress run-ct",
    "pretest:e2e": "npm run build",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "pretest:e2e:ci": "npm run build",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@gouvminint/vue-dsfr": "^1.8.0",
    "focus-trap": "^6.8.1",
    "focus-trap-vue": "^3.2.1",
    "oh-vue-icons": "1.0.0-rc3",
    "pinia": "^2.0.28",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^5.0.2",
    "@cypress/vue": "^5.0.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "2.2.7",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.3.0",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^21.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "start-server-and-test": "^1.15.2",
    "typescript": "~4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "vue-tsc": "^1.0.24",
    "vitest": "^0.26.3",
    "workbox-window": "^6.5.4"
  }
}

