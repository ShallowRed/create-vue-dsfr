{
  "name": "vue-dsfr-nuxt-ts-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "test": "run-s test:unit test:ct-ci test:e2e-ci",
    "test:unit": "vitest --environment jsdom",
    "test:ct": "cypress open-ct",
    "test:ct:ci": "cypress run-ct",
    "test:e2e": "start-server-and-test dev http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test dev http://127.0.0.1:3000/ 'cypress run'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@laruiss/vue-dsfr": "^1.0.0-beta.51"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@rushstack/eslint-patch": "^1.1.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/user-event": "^14.1.0",
    "@testing-library/vue": "^6.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.5.4",
    "eslint": "^8.13.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.3",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.6"
  }
}
