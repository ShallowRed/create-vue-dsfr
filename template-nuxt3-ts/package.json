{
  "name": "vue-dsfr-nuxt-ts-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test dev http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test dev http://127.0.0.1:3000/ 'cypress run'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@laruiss/vue-dsfr": "^1.0.0-beta.42"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/user-event": "^13.5.0",
    "@testing-library/vue": "^6.5.1",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.22",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^5.8.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.4.1",
    "eslint": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "nuxt3": "latest",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.5.5",
    "vitest": "^0.2.5",
    "nuxt3": "latest"
  }
}
